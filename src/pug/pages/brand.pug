      section.row-arrow
        .row-arrow__container
          .container
            .row-arrow__wrapper
              .row-arrow__block
                span.row-arrow__text Смотреть все
                a.arrow-to-scroll.arrow-to-scroll--black(href="")
                  svg.arrow-to-scroll__arrow
                    use(xlink:href="img/sprite.svg#arrow-scroll")
                  .arrow-to-scroll__wrap
                    svg.arrow-to-scroll__line
                      use(xlink:href="img/sprite.svg#line")
                    
      section.developments.black-bg.padding-top.padding-bottom
        .container
          .developments__container
            .row-title
              h2.title Актуальные предложения от Audi
            include includes/developments.pug
      
      section.connect(style="background-image: url(img/connect-bg2.jpg)")
        .container
          .connect__container
            .connect__block
              .row-title
                h2.title.title--white Подпишитесь на спецпредложения и будьте в курсе всех акций
              a.block-btn.block-btn--white(href="#")
                .block-btn__line.block-btn__line--top
                .block-btn__line.block-btn__line--right
                .block-btn__line.block-btn__line--bottom
                .block-btn__line.block-btn__line--left
                button.block-btn__btn Подписаться

      section.gray-bg
        .container
          .gray-bg__wrapper
            .centers
              .row-title
                h2.title Дилерские центры Audi
              .centers__wrapper
              
                .centers-row
                  .centers-row__left-block
                    .centers-row__row
                      img.centers-row__img(src="img/centers-img.png")
                      .centers-row__wrap
                        h3.centers-row__title Авторизованный дилерский центр Audi Тула
                        a.centers-row__link(href="mailto:")
                          .centers-row__arrow
                            svg.centers-row__svg
                              use(xlink:href="img/sprite.svg#sm-arrow-right")
                          span.centers-row__link-text www.audi-tula.ru
                  .centers-row__center-block                    
                    .centers-row__block-text
                      .centers-row__wrap-svg
                        svg.centers-row__location
                          use(xlink:href="img/sprite.svg#location")
                      span.centers-row__text 300012, г. Тула, ул. Рязанская, д. 7
                      span.centers-row__text Ежедневно с 08:00 до 21:00 без перерыва и выходных
                    a.centers-row__link(href="#")
                      .centers-row__arrow
                        svg.centers-row__svg
                          use(xlink:href="img/sprite.svg#sm-arrow-right")
                      span.centers-row__link-text Показать на карте
                  .centers-row__right-block                    
                    .centers-row__block-text
                      .centers-row__wrap-svg
                        svg.centers-row__tell
                          use(xlink:href="img/sprite.svg#tell")
                      a.centers-row__tel(href="tel:") 8-800-777-17-47
                      span.centers-row__sm-text Связь с менеджером
                    a.centers-row__link(href="#")
                      .centers-row__arrow
                        svg.centers-row__svg
                          use(xlink:href="img/sprite.svg#sm-arrow-right")
                      span.centers-row__link-text Обратный звонок
                      
